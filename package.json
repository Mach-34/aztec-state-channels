    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@aztec/accounts": "file:../aztec-packages/yarn-project/accounts",
    "@aztec/acir-simulator": "file:../aztec-packages/yarn-project/acir-simulator",
